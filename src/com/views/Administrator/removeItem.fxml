<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RemoveItemController">
   <children>
      <Button fx:id="goBackButton" layoutX="35.0" layoutY="56.0" mnemonicParsing="false" onAction="#switchToInventory" text="Go Back" />
      <Button fx:id="removeButton" layoutX="35.0" layoutY="97.0" mnemonicParsing="false" onAction="#removeAction" text="Remove" />
      <TableView fx:id="tableView" layoutX="115.0" layoutY="19.0" prefHeight="360.0" prefWidth="465.0">
        <columns>
          <TableColumn fx:id="checkBoxes" prefWidth="50.0" />
          <TableColumn fx:id="products" prefWidth="400.0" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
