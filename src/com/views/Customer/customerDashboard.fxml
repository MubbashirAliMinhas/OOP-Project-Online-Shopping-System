<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CustomerDashboardController">
   <children>
      <Button layoutX="548.0" layoutY="1.0" mnemonicParsing="false" onAction="#closeWindow" styleClass="button-close" stylesheets="@styles.css" text="CLOSE" />
      <VBox layoutX="107.0" layoutY="100.0" prefHeight="200.0" prefWidth="399.0" styleClass="vbox" stylesheets="@styles.css">
         <children>
            <Label fx:id="welcomeLabel" text="Label" />
            <Label fx:id="visitsLabel" text="Label" />
            <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#switchToLogIn" styleClass="button-login" stylesheets="@styles.css" text="LOG OUT" />
         </children>
      </VBox>
   </children>
</AnchorPane>
